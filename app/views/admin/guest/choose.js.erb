$('#table-guest').html("<%= escape_javascript(render 'admin/guest/component/table.html') %>");

$("#add-guest-aksen").on("click",function(){
	$("#import-guest").show();
	$("#table-guest").hide();	
});

$("#send-email").on("click",function(){
	let id = $("#wedding option:selected").val();
	$.ajax({
		url: '/admin/guest/send_mail',
		method: 'get',
		data: {wedding_id:id}
	});
});