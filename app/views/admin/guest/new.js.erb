$("#back-import-guest").on("click",function(){
	$("#import-guest").hide();
	$('#table-guest').html("<%= escape_javascript(render 'admin/guest/component/table.html') %>");
	$('#table-guest').show();	
});

$("#add-guest-aksen").on("click",function(){
	$("#import-guest").show();
	$("#table-guest").hide();	
});