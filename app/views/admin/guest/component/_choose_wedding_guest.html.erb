<div class="row" id="choose-wedding-guest">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>List wedding <small>Sessions</small></h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a id='back-to-list' ><i class="fa fa-chevron-left"></i></a></li>
          <li><a class="close-link"><i class="fa fa-close"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="x_content">
      	<%= form_tag( choose_admin_guest_index_path, class: 'form-horizontal form-label-left', method:'get', remote:true)  do %>
      		<div class="item form-group">
      			<%= label_tag('name_wedding', 'Pilih Wedding', class: "control-label col-md-3 col-sm-3 col-xs-12") %>
            <div class="col-md-6 col-sm-6 col-xs-12">
              <%= select_tag("wedding",options_from_collection_for_select(@weddings, "id", "name_wedding"), class: 'form-control')%>
            </div>
      		</div>
      		<div class="ln_solid"></div>
      		<div class="form-group">
      			<%= button_tag("Submit", class: "btn btn-primary") %>
      		</div>
      	<% end %>
      </div>
    </div>
  </div>
</div>