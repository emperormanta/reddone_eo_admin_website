<div class="row" id="table-guest-aksen">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>List wedding <small>Sessions</small></h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a id="add-guest-aksen" ><i class="fa fa-plus-square"></i></a></li>
          <li><a class="close-link"><i class="fa fa-close"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
	    <div class="x_content" id="reload">
				<table id="data-table" class="table table-striped table-bordered">
					<thead>
						<tr>
							<th>Guest Id</th>
							<th>Name</th>
							<th>Email</th>
							<th>address</th>
							<th>presence</th>
							<th>note</th>
							<th>url</th>
							<th>Wedding Id</th>
							<th>Action</th>
						</tr>
					</thead>
					<tbody>
						<% @guests.each do |guest| %>
							<tr>
								<td><%= guest.guest_id %></td>
								<td><%= guest.name %></td>
								<td><%= guest.email %></td>
								<td><%= guest.address %></td>
								<td><%= guest.presence %></td>
								<td><%= guest.note %></td>
								<td><%= guest.url %></td>
								<td><%= guest.wedding_id %></td>
								<td><a id="<%= guest.id %>" class="fa fa-edit"></a> || 
									<%= link_to '', admin_wedding_path(guest.id), class: "fa fa-trash", method: :delete, data: { confirm: 'Are you sure?'}  %></td>
							</tr>
						<% end %>
					</tbody>
				</table>				
      </div>
    </div>
  </div>
</div>


