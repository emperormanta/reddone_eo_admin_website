<div class="right_col" role="main" style="min-height: 3790px;">
	<div class="">
	  
	  <div class="page-title">
	    <div class="title_left">
	      <h3>Wedding</h3>
	    </div>

	    <div class="title_right">
	      <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
	        <div class="input-group">
	          <input type="text" class="form-control" placeholder="Search for...">
	          <span class="input-group-btn">
	            <button class="btn btn-default" type="button">Go!</button>
	          </span>
	        </div>
	      </div>
	    </div>
	  </div>

	  <div class="clearfix"></div>

	  <div class="row" id="table-wedding">
	    <div class="col-md-12 col-sm-12 col-xs-12">
	      <div class="x_panel">
	        <div class="x_title">
	          <h2>List wedding <small>Sessions</small></h2>
	          <ul class="nav navbar-right panel_toolbox">
	            <li><a id="add-wedding" ><i class="fa fa-plus-square"></i></a></li>
	            <li><a class="close-link"><i class="fa fa-close"></i></a>
	            </li>
	          </ul>
	          <div class="clearfix"></div>
	        </div>
	        <div class="x_content" id="reload">
	        	<%= render 'admin/wedding/component/table.html' %>
	        </div>
	      </div>
	    </div>
	  </div>

	  <%= render 'admin/wedding/component/add_form.html' %>
	  <%= render 'admin/wedding/component/edit_form.html' %>

	</div>
</div>






