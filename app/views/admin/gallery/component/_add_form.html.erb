<div class="row" id="form-add-gallery">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>List wedding <small>Sessions</small></h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a id='back-to-list' ><i class="fa fa-chevron-left"></i></a></li>
          <li><a class="close-link"><i class="fa fa-close"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="x_content">
      	<%= form_tag('/admin/gallery', class: 'form-horizontal form-label-left', method:'post', remote:true)  do %>
      		<div class="item form-group">
      			<%= label_tag('image_file_location', 'Location File', class: "control-label col-md-3 col-sm-3 col-xs-12") %>
      			<div class="col-md-6 col-sm-6 col-xs-12">
      				<%= text_field_tag("image_file_location",nil, class: 'form-control')%>
      			</div>
      		</div>
      		<div class="item form-group" >
        		<%= label_tag('image_description', 'Description', class: "control-label col-md-3 col-sm-3 col-xs-12") %>
        		<div class="col-md-6 col-sm-6 col-xs-12">
	        		<%= text_area_tag("owner",nil, class: "form-control") %>
            </div>
        	</div>
          <div class="item form-group">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Active</label>
            <div class="col-md-6 col-sm-6 col-xs-12">
              <div class="">
                <label>
                  <input type="checkbox" class="js-switch" name="active" value=true checked="checked" /> active
                </label>
              </div>
            </div>
          </div>
          <div class="item form-group" >
            <%= label_tag('owner', 'Owner', class: "control-label col-md-3 col-sm-3 col-xs-12") %>
            <div class="col-md-6 col-sm-6 col-xs-12">
              <%= text_field_tag("owner",nil, class: "form-control") %>
            </div>
          </div>
      		<div class="ln_solid"></div>
      		<div class="form-group">
      			<%= button_tag("Submit", class: "btn btn-primary") %>
      		</div>
      	<% end %>
      </div>
    </div>
  </div>
</div>